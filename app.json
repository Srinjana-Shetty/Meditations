{
  "expo": {
    "name": "Meditations",
    "slug": "Meditations",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/simpleMeditationLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.srinjana.Meditations",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.srinjana.Meditations"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/simpleMeditationLogo.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
          
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "751c165a-f489-4ca3-a89b-fe251d5af691"
      }
    }
  }
}
